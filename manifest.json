{
  "name": "TikTok All Reposted Videos Remover",
  "description": "A Chrome extension to remove all reposted videos on TikTok.",
  "version": "1.2.1",
  "author": {
    "name": "Nazky Narutama",
    "email": "naruzky.naz@gmail.com",
    "url": "github.com/444Nazky"
  },
  "homepage_url": "https://chromewebstore.google.com/detail/tiktok-all-reposted-videos-remover/amgpfdpibiacligkkkbeonfhmonkgjhg",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://*.tiktok.com/*"],

  "background": {
    "service_worker": "dist/background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "TikTok All Reposted Video Remover",
    "default_popup": "popup.html"
  },

  "web_accessible_resources": [
    {
      "resources": ["dist/script.js", "styles.css"],
      "matches": ["https://*.tiktok.com/*"]
    }
  ],
  "manifest_version": 3,
  "default_locale": "en"
}
